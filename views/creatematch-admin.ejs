<%- include('./partials/head.ejs') %>
<body>
<header>
    <a href="/admin">terug</a>
</header>
<main>
    <form action="/admin/creatematch" method="post">
        <input type="date" name="speeldag" value="<%= matchData.speeldag %>">
        <input type="time" name="speeltijd" value="<%= matchData.speeltijd %>">
        <input type="number" name="thuisscore" value="<%= matchData.thuisscore %>">
        <input type="number" name="uitscore" value="<%= matchData.uitscore %>">
        <input type="text" name="thuisnaam" placeholder="thuis" value="<%= matchData.thuisnaam %>">
        <input type="text" name="uitnaam" placeholder="uit" value="<%= matchData.uitnaam %>">
        <input type="submit" value="verstuur score">
    </form>
</main>
</body>
<%- include('./partials/footer.ejs') %>